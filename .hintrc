{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content"
        ]
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 122",
    "not firefox <= 123"
  ]
}